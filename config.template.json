{
    // The canonical domain for the app, used for webhook verification and oauth callbacks
    "domain": "kitchenview2.newpointe.org",

    // The port to run on.
    "port": 8080,

    // A random secret key to use for cookie/session encryption
    "cookieSecret": "asdfg",

    "db": {

        // The database type. Can be "mysql", "sqlite", "postgres", or "mssql".
        // You will also need to make sure the appropriate database drivers are installed: "npm i mysql2", "npm i sqlite3", "npm i pg", or "npm i tedious"
        "dialect": "mysql",

        "host": "localhost",

        "port": 3306,

        "database": "kitchenview",

        "username": "kitchenview",

        "password": "asdfg",

        // For sqlite, the file for the database.
        "storage": "path/to/database.sqlite"

    },
    "square": {

        // The Client Id for your app
        "clientID": "asdfg",

        // The Client Secret for your app
        "clientSecret": "asdfg",

        // The callback url for your app. Should be https://yourdomain/auth/square/callback.
        "callbackURL":"http://localhost:3000/auth/square/callback",

        // The signature key for webhooks
        "signatureKey": "asdfg"

    }
}
